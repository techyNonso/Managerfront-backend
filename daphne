[Unit]
Description=WebSocket Daphne Service
After=network.target

[Service]
Type=simple
User=chukwunonso
WorkingDirectory=/home/chukwunonso/managerfrontFolder/Managerfront-backend
ExecStart=/home/chukwunonso/managerfrontFolder/env/bin/python /home/chukwunonso/managerfrontFolder/env/bin/daphne -b 0.0.0.0 -p 8001 managerfront.asgi:application
Restart=on-failure

[Install]
WantedBy=multi-user.target